{
  "name": "soss-v2",
  "version": "1.0.0",
  "description": "Sidebrief backend version 2",
  "main": "index.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "jest --forceExit --detectOpenHandles --runInBand",
    "prisma": "npx prisma generate --schema prisma/schema1.prisma && npx prisma generate --schema prisma/schema2.prisma",
    "dev": "npx prisma migrate dev --schema prisma/schema2.prisma && npx prisma migrate dev --schema prisma/schema1.prisma && npm run build && node dist/index.js",
    "deploy": "npm run prisma && npx prisma migrate deploy --schema prisma/schema2.prisma && npx prisma migrate deploy --schema prisma/schema1.prisma && npm run build && cp -r prisma dist/ && node dist/index.js",
    "start": "npm run deploy"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.359.0",
    "@prisma/client": "^4.14.1",
    "@types/ejs": "^3.1.3",
    "@types/express-session": "^1.17.8",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/passport": "^1.0.13",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.4",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "fastest-validator": "^1.17.0",
    "googleapis": "^120.0.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "nodemailer": "^6.9.2",
    "nodemon": "^3.0.3",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "winston": "^3.9.0",
    "winston-cloudwatch": "^6.1.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/nodemailer": "^6.4.11",
    "@types/supertest": "^2.0.14",
    "jest": "^29.5.0",
    "prisma": "^4.16.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
